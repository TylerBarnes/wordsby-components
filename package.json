{
  "name": "wordsby-components",
  "version": "0.0.21",
  "description": "Components for wordsby",
  "main": "index.js",
  "author": "Tyler Barnes <tyler@known.design>",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-npm-files": "cp -rf package.json ./lib/ && cp -rf readme.md ./lib/",
    "clean-lib": "node_modules/.bin/rimraf ./lib",
    "build": "rm -rf src/package.json src/node_modules src/yarn.lock || true && yarn clean-lib && babel src --out-dir lib --ignore __tests__ && yarn copy-npm-files",
    "watch": "npm run clean-lib && mkdir lib && npm run copy-npm-files && babel -w src --out-dir lib --ignore __tests__"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "babel-preset-gatsby-package": "^0.1.3",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "wordsby": "^0.5.0"
  },
  "dependencies": {
    "gatsby-image": "^2.0.25",
    "iframe-resizer": "^3.6.3",
    "post-robot": "^9.0.19",
    "url-parse": "^1.4.4"
  }
}
